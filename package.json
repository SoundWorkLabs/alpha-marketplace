{
    "name": "mantine-minimal-next-template",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint --fix",
        "format:all": "prettier --write app/",
        "format:staged": "prettier --write $(git diff --staged --name-only --diff-filter d | xargs)"
    },
    "dependencies": {
        "@mantine/core": "7.0.0",
        "@mantine/dropzone": "^7.0.0",
        "@mantine/form": "^7.0.0",
        "@mantine/hooks": "7.0.0",
        "@metaplex-foundation/umi": "^0.8.9",
        "@metaplex-foundation/umi-bundle-defaults": "^0.8.9",
        "@metaplex-foundation/umi-web3js-adapters": "^0.8.9",
        "@project-serum/anchor": "^0.26.0",
        "@project-serum/sol-wallet-adapter": "^0.2.6",
        "@solana/wallet-adapter-base": "^0.9.23",
        "@solana/wallet-adapter-react": "^0.15.35",
        "@solana/wallet-adapter-react-ui": "^0.9.34",
        "@solana/wallet-adapter-wallets": "^0.19.22",
        "@solana/web3.js": "^1.78.5",
        "@tabler/icons-react": "^2.34.0",
        "bs58": "^5.0.0",
        "install": "^0.13.0",
        "next": "13.4.4",
        "pnpm": "^8.8.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hot-toast": "^2.4.1",
        "tailwindcss-gradients": "^3.0.0"
    },
    "devDependencies": {
        "@jimii/soundwork-sdk": "^0.0.6",
        "@types/node": "20.2.5",
        "@types/react": "18.2.7",
        "@types/react-dom": "18.2.4",
        "@typescript-eslint/eslint-plugin": "^6.7.4",
        "autoprefixer": "^10.4.15",
        "axios": "^1.5.1",
        "eslint": "^8.50.0",
        "eslint-config-next": "^13.5.4",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "husky": "^8.0.3",
        "postcss": "^8.4.30",
        "postcss-preset-mantine": "1.6.0",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.0.3",
        "tailwindcss": "^3.3.3",
        "typescript": "5.0.4"
    },
    "lint-staged": {
        "*.{js,jsx,css,md,json}": "prettier --write --cache",
        "*.{ts,tsx}": [
            "prettier --write --cache",
            "eslint --fix --cache"
        ]
    }
}
